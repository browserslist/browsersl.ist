{
  "name": "browserl.ist",
  "private": true,
  "homepage": "https://browsersl.ist/",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=20"
  },
  "scripts": {
    "start": "pnpm -r start",
    "prebuild": "pnpm -r prebuild",
    "build": "pnpm -r build",
    "test:js": "eslint .",
    "test:css": "stylelint **/*.css",
    "test:checks": "node scripts/check-versions.js",
    "test:audit": "pnpm audit --prod",
    "test": "pnpm run prebuild && pnpm build && pnpm run --recursive --include-workspace-root /^test:/"
  },
  "dependencies": {
    "ssdeploy": "^0.9.3"
  },
  "devDependencies": {
    "@logux/eslint-config": "^52.0.1",
    "@logux/stylelint-config": "^0.16.0",
    "eslint": "^8.53.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-node-import": "^1.0.4",
    "eslint-plugin-perfectionist": "^2.3.0",
    "eslint-plugin-prefer-let": "^3.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "nano-staged": "^0.8.0",
    "prettier": "^3.0.3",
    "simple-git-hooks": "^2.9.0",
    "stylelint": "^15.11.0",
    "svgo": "^3.0.3"
  }
}
