{
  "name": "browserl.ist",
  "private": true,
  "homepage": "https://browsersl.ist/",
  "type": "module",
  "license": "MIT",
  "engines": {
    "node": ">=24"
  },
  "packageManager": "pnpm@10.29.3",
  "scripts": {
    "start": "pnpm -r start",
    "prebuild": "pnpm -r prebuild",
    "build": "pnpm -r build",
    "test:js": "eslint .",
    "test:css": "stylelint **/*.css",
    "test:checks": "node scripts/check-versions.js",
    "test:audit": "pnpm audit --prod",
    "test": "pnpm run prebuild && pnpm build && pnpm run --recursive --include-workspace-root /^test:/"
  },
  "devDependencies": {
    "@logux/eslint-config": "^57.0.1",
    "@logux/stylelint-config": "^0.21.0",
    "actions-up": "^1.9.0",
    "eslint": "^9.39.2",
    "globals": "^17.0.0",
    "nano-staged": "^0.9.0",
    "prettier": "^3.7.4",
    "simple-git-hooks": "^2.13.1",
    "stylelint": "^16.26.1",
    "svgo": "^4.0.0"
  }
}
